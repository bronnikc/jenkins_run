pipeline {
    agent any
	parameters
			{
			  string(name: 'ORACLE_HOME', defaultValue: 'db_home1', description: 'The target Oracle_HOME')
			}
	stages{
		stage('Read Jenkinsfile') 
        {
            steps {
                     script 
                     {
                         ORACLE_HOME='gg'
                     }  
                  }
        }
        stage('Show d')
        {
            steps
            {
                echo  "${ORACLE_HOME}"
            }
        }
    }
}
