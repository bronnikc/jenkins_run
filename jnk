pipeline {
    agent any
	def choice = input message: '<message>',
    parameters: [choice(choices: "choice1\nchoice2\nchoice3\nchoice4\n",
    description: 'Choose an option', name: 'Options')]
    parameters
			{
			  string(name: 'ORACLE_HOME', defaultValue: 'db_home1', description: 'The target Oracle_HOME')
			}
	stages{
		stage('Read Jenkinsfile') 
        {
            steps {
                     script 
                     {
                         ORACLE_HOME='gg'
                     }  
                  }
        }
        stage('Show d')
        {
            steps
            {
                echo  "${ORACLE_HOME}"
            }
        }
    }
}
